package com.vivebest.erp.entity;

import java.math.BigDecimal;
import java.util.Date;


public class TransferInfo {
	private int tiId; // 转账id
	private String trNumber; // 转账流水号
	private String tiOutNum; // 出账账户
	private String tiInNum; // 入账账户
	private BigDecimal tiBalance; // 转账金额
	private Date tiTime;
	private boolean tiState; // 转账状态
	private String tiResult; // 转账结果
	private boolean isDelete; // 软删除

	public TransferInfo() {
		super();
		// TODO Auto-generated constructor stub
	}

	public TransferInfo(int tiId, String trNumber, String tiOutNum,
			String tiInNum, BigDecimal tiBalance, Date tiTime, boolean tiState,
			String tiResult, boolean isDelete) {
		super();
		this.tiId = tiId;
		this.trNumber = trNumber;
		this.tiOutNum = tiOutNum;
		this.tiInNum = tiInNum;
		this.tiBalance = tiBalance;
		this.tiTime = tiTime;
		this.tiState = tiState;
		this.tiResult = tiResult;
		this.isDelete = isDelete;
	}

	public int getTiId() {
		return tiId;
	}

	public void setTiId(int tiId) {
		this.tiId = tiId;
	}

	public String getTrNumber() {
		return trNumber;
	}

	public void setTrNumber(String trNumber) {
		this.trNumber = trNumber;
	}

	public String getTiOutNum() {
		return tiOutNum;
	}

	public void setTiOutNum(String tiOutNum) {
		this.tiOutNum = tiOutNum;
	}

	public String getTiInNum() {
		return tiInNum;
	}

	public void setTiInNum(String tiInNum) {
		this.tiInNum = tiInNum;
	}

	public BigDecimal getTiBalance() {
		return tiBalance;
	}

	public void setTiBalance(BigDecimal tiBalance) {
		this.tiBalance = tiBalance;
	}

	public Date getTiTime() {
		return tiTime;
	}

	public void setTiTime(Date tiTime) {
		this.tiTime = tiTime;
	}

	public boolean getTiState() {
		return tiState;
	}

	public void setTiState(boolean tiState) {
		this.tiState = tiState;
	}

	public String getTiResult() {
		return tiResult;
	}

	public void setTiResult(String tiResult) {
		this.tiResult = tiResult;
	}

	public boolean isDelete() {
		return isDelete;
	}

	public void setDelete(boolean isDelete) {
		this.isDelete = isDelete;
	}

	@Override
	public String toString() {
		return "TransferInfo [tiId=" + tiId + ", trNumber=" + trNumber
				+ ", tiOutNum=" + tiOutNum + ", tiInNum=" + tiInNum
				+ ", tiBalance=" + tiBalance + ", tiTime=" + tiTime
				+ ", tiState=" + tiState + ", tiResult=" + tiResult
				+ ", isDelete=" + isDelete + "]";
	}

}
